hist(vctMeans)
meanHeight <- mean(vctMeans)
sdHeight <- sd(vctMeans)
moe <- 1.645 * sdHeight        # Calculate the margin of error and store in the variable 'moe'
negCI <- meanHeight - moe     #low CI value = mean - moe
posCI <- meanHeight + moe
print(meanHeight)
print(negCI)
print(posCI)
